[Unit]
Description=Frp Client Service
After=network.target

[Service]
Type=idle
User=nobody
LimitCORE=infinity
LimitNOFILE=100000
LimitNPROC=100000
Restart=on-failure
RestartSec=5s
ExecStart=/usr/bin/frpc -c /etc/frp/%i.ini
ExecReload=/usr/bin/frpc reload -c /etc/frp/%i.ini

[Install]
WantedBy=multi-user.target
