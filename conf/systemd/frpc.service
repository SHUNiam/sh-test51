[Unit]
Description=Frp Client Service
After=network.target

[Service]
Type=simple
User=nobody
LimitCORE=infinity
LimitNOFILE=65535
LimitNPROC=65535
Restart=on-failure
RestartSec=5s
ExecStart=/usr/bin/frpc -c /etc/frp/frpc.ini
ExecReload=/usr/bin/frpc reload -c /etc/frp/frpc.ini

[Install]
WantedBy=multi-user.target
